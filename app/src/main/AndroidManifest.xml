<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.workout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WeekOneActivity"
            android:label="@string/week_one"
            android:screenOrientation="portrait"/>
        <activity android:name=".WeekTwoActivity"
            android:label="@string/week_two"
            android:screenOrientation="portrait"/>
        <activity android:name=".WeekThreeActivity"
            android:label="@string/week_three"
            android:screenOrientation="portrait"/>
        <activity android:name=".WeekFourActivity"
            android:label="@string/week_four"
            android:screenOrientation="portrait"/>
        <activity android:name=".WeekFiveActivity"
            android:label="@string/week_five"
            android:screenOrientation="portrait"/>
        <activity android:name=".WeekSixActivity"
            android:label="@string/week_six"
            android:screenOrientation="portrait"/>
    </application>

</manifest>